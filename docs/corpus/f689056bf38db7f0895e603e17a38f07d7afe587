mkdir(&(0x7f0000000240)='./file1\x00', 0x0)
mkdir(&(0x7f0000000580)='./file0\x00', 0x0)
mkdir(&(0x7f0000000040)='./bus\x00', 0x0)
r0 = creat(&(0x7f0000000040)='./bus/file0\x00', 0x0)
r1 = openat$dir(0xffffffffffffff9c, &(0x7f0000000380)='./file0\x00', 0x0, 0x0)
linkat(r0, &(0x7f0000000080)='\x00', r1, &(0x7f0000000000)='./file1\x00', 0x1400)
mount$overlay(0x400002, &(0x7f0000000180)='./bus\x00', &(0x7f0000000100)='overlay\x00', 0x0, &(0x7f0000000400)=ANY=[@ANYBLOB='upperdir=./file1,lowerdir=./bus,workdir=./file0,metacopy=on,index=on'])
truncate(&(0x7f0000003400)='./bus/file0\x00', 0x0)
unlink(&(0x7f0000000140)='./bus/file0\x00')
