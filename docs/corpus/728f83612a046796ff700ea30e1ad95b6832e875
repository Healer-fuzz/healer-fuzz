mknod(&(0x7f0000000200)='./bus\x00', 0x1000, 0x0)
mknod(&(0x7f0000000040)='./file0\x00', 0x8001420, 0x0)
r0 = open$dir(&(0x7f0000000100)='./file0\x00', 0x2, 0x0)
r1 = open(&(0x7f0000000080)='./bus\x00', 0x42202, 0x0)
splice(r0, 0x0, r1, 0x0, 0xffffffe1, 0x0)
r2 = open$dir(&(0x7f0000000180)='./file0\x00', 0x7e, 0x0)
read$FUSE(r1, &(0x7f00000004c0)={0x2020}, 0x2020)
r3 = dup2(r2, r1)
write$FUSE_IOCTL(r3, &(0x7f0000000000), 0x20)
